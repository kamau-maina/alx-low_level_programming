#include "main.h"

/*
 *times_table - prints 9 times table starting with 0
 *Return: always 0;
 */
void times_table(void)
{       
        int a;
        int b;
        int c;
        for (a = 0; a <= 9; a++)
        {       
                for (b = 0; b <= 10; b++)
                {       
                        c=a*b;
                        printf("%d",c);
                        printf(" ");
                }  
        printf("\n");     
        }       
}  

